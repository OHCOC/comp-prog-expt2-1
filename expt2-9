#include <iostream>
#include <string>

using namespace std;

int main ()
{
	int Row;
	int Column;
	
	cout << " How many rows do you want? \n";
	cin >> Row;
	cout << " How many columns do you want? \n";
	cin >> Column;
	
	for (int rows=1; rows <=Row; ++rows)
	{
		for (int col=1; col <=Column; ++col)
	{
		cout <<" * ";
	}
	cout << endl;
}
return 0;	
}
